<header id="main_header" class="cf">
	<div class="wrapper_wide">
		<%= link_to "MusicFlix", root_path, id: "logo" %>
		<nav>
			<ul>
				<% unless music_signed_in? %>
					<li><%= link_to "Explore", musics_path %></li>
					<li><%= link_to "Sign In", new_music_session_path %></li>
					<li><%= link_to "Upload Music", new_music_registration_path %></li>
				<% else %>
					<li><%= link_to "Dashboard", root_path %></li>
					<li><%= link_to "Explore", musics_path %></li>
					<li><%= link_to "Sign Out", destroy_music_session_path, method: :delete %></li>
				<% end %>
			</ul>
		</nav>
	</div>
</header>